{
  "name": "kazemi-shioru-fusion",
  "version": "1.0.0",
  "description": "Discord Bot Fusion - Kazemi AI + Shioru Features",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp -r src/generated dist/",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts,json --exec \"npm run build && node dist/index.js\"",
    "deploy": "npm run build && npm run start",
    "lint": "eslint --fix --ext .ts . && prettier --check --write .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@distube/deezer": "^2.0.1",
    "@distube/soundcloud": "^2.0.4",
    "@distube/spotify": "^2.0.2",
    "@distube/youtube": "^1.0.4",
    "@distube/yt-dlp": "^2.0.1",
    "@google/generative-ai": "^0.22.0",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@snazzah/davey": "^0.1.9",
    "captcha-canvas": "^4.1.0",
    "discord-gamecord": "^4.4.2",
    "discord.js": "^14.25.1",
    "distube": "^5.0.7",
    "dotenv": "^16.4.7",
    "genius-lyrics": "^4.4.7",
    "i18next": "^25.3.1",
    "i18next-fs-backend": "^2.6.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^7.2.0",
    "systeminformation": "^5.27.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "firebase": "*",
    "ffmpeg": "*"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "generative-ai",
    "music",
    "games",
    "i18n",
    "fusion"
  ],
  "author": "",
  "license": "MIT"
}
